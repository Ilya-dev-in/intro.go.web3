{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "server",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "main.go"
        },
        {
            "name": "Chrome",
            "request": "launch",
            "type": "chrome",
            "url": "http://localhost:5173",
            "webRoot": "${workspaceRoot}\\webapp"
        },
        {
            "command": "npm run dev",
            "name": "Webapp",
            "request": "launch",
            "type": "node-terminal",
            "cwd": "${workspaceFolder}\\webapp"
        },
    ]
}